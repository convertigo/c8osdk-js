{
  "name": "c8osdkvuejs",
  "version": "2.1.0",
  "description": "convertigo's MBaaS client API service",
  "main": "bundle/index.umd.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src/",
    "LICENCE",
    "tsconfig.json"
  ],
  "scripts": {
    "publish:beta": "npm publish --tag beta",
    "publish": "npm publish",
    "test": "karma start",
    "test-watch": "karma start --singleRun=false --autoWatch=true --captureExceptions",
    "clean": "rimraf bundles coverage src/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngsummary.json src/index.d.ts src/index.js src/index.js.map",
    "prepare": "npm run clean && tsc -p . && npm run build",
    "build": "webpack && cp bundles/index.umd.js bundles/index.js",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/c8o/",
    "typedoc": "typedoc",
    "build-dev": "tsc -p ."
  },
  "keywords": [
    "MBaaS",
    "TypeScript",
    "JavaScript",
    "Vue.js",
    "Convertigo",
    "mbaas",
    "madp",
    "mobility",
    "nosql",
    "couchdb"
  ],
  "author": {
    "name": "Convertigo",
    "email": "info@convertigo.com",
    "url": "http://www.convertigo.com/"
  },
  "license": "Apache License Version 2.0, January 2004",
  "dependencies": {
    "@types/node": "^8.5.2",
    "@types/pouchdb": "6.3.2",
    "pouchdb-browser": "6.4.3",
    "rxjs": "5.5.10",
    "zone.js": "0.8.26",
    "axios": "0.18.0",
    "c8osdkjscore": "1.0.0"
  },
  "devDependencies": {
    "rimraf": "2.6.1",
    "@types/jasmine": "2.8.2",
    "awesome-typescript-loader": "3.4.1",
    "codelyzer": "4.0.1",
    "commitizen": "2.9.6",
    "concurrently": "3.5.1",
    "core-js": "2.5.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.6",
    "loader-utils": "1.1.0",
    "reflect-metadata": "0.1.10",
    "source-map-loader": "0.2.3",
    "ts-helpers": "1.1.2",
    "tsify": "4.0.0",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "typedoc": "0.9.0",
    "webpack": "3.10.0",
    "typescript": "2.6.2",
    "browserify": "16.2.0"
  }
}
